{
  "name": "this-app-is-garbage-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "cross-env CI=true jest --silent --passWithNoTests --notify",
    "setup-husky": "husky install",
    "staged-pretty-css": "pretty-quick --staged  --pattern \"**/*.css\" \"**/*.scss\"",
    "staged-pretty-js": "pretty-quick --staged  --pattern \"**/*.js\" \"**/*.jsx\" \"**/*.json\"",
    "test:jest": "jest --notify",
    "precommit": "npx lint-staged",
    "pretty": "prettier --use-tabs --single-quote --write \"**/*.{js,jsx}\"",
    "verify-node-version": "node ./bin/verify-node-version.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "aos": "^2.3.4",
    "axios": "^1.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "next": "12.3.1",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "cross-env": "^7.0.3",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "node-notifier": "^10.0.1",
    "prettier": "^2.7.1",
    "prettier-cli": "^0.1.0",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "pretty-quick": "^3.1.3"
  }
}
